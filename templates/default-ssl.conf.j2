<IfModule mod_ssl.c>
  <VirtualHost _default_:{{apache2_default_https_port}}>

    ServerAdmin webmaster@localhost
    DocumentRoot {{apache2_base_path}}

    ErrorLog {{apache2_log_path}}/error.log
    CustomLog {{apache2_log_path}}/access.log combined

    SSLEngine on

    {% if apache2_certificate_file is defined -%}
    SSLCertificateFile      {{apache2_certificate_file}}
    {% endif -%}
    {% if apache2_certificate_key_file is defined -%}
    SSLCertificateKeyFile   {{apache2_certificate_key_file}}
    {% endif %}

    <Directory "{{apache2_base_path}}">
      Options Indexes FollowSymLinks
      AllowOverride None
      Require all granted
    </Directory>
  </VirtualHost>
</IfModule>